00000, // A		->	0x000000, // B		->	0x000000, // C		->	0x000000, // D		->	0x000000, // Betty	->	0x000000, // Exit	->		0x000000, // Up		->		0x000000, // Down	->		0x000000, // Left	->		0x000000, // Right	->		0x000000, // OK		->		0x000000, // Vol+	->	0x000000, // Vol-	->	0x000000, // Mute	->	0x000000, // Prog+	->	0x000000, // Prog-	->	0x000015, // 1		-> IT-V1-code e.g. A1 on , 00000000 0F FF(binary: 000000000000000000010101 0x000015 	0x000014, // 2		-> A1 off	0x000000, // 3		-> 	0x000000, // 4		-> 	0x000000, // 5		-> 	0x000000, // 6		-> 	0x000000, // 7		-> 	0x000000, // 8		-> 	0x000000, // 9		-> 	0x000000, // 0		-> 	0x000000, // -/--	->		0x000000, // AV		->	0x000000, // Menu	-> 	0x000000, // PiP	-> 	0x000000, // A/B	-> 	0x000000, // 16:9	-> 		0x000000, // Info	-> 		0x000000, // VTX1	-> 	0x000000, // VTX2	-> 		0x000000, // VTX3	-> 		0x000000, // Blue	->		0x000000, // Yello	->		0x000000, // Green	->		0x000000, // Red	->		0x000000, // TV		->		0x000000  // Power	->	Power},