->	Info	0xD629, // VTX1		->	Ch.List	0x34cb, // VTX2		->	vtx	0xC837, // VTX3		->	Pre-CH	0x6897, // Blue		->	Blue	0xA857, // Yello	->	Yellow	0x28D7, // Green	->	Green	0x36C9, // Red		->	Red	0xC23D, // TV		->	TV/DTV	0x40BF  // Power	->	Standby},