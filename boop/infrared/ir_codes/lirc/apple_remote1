x00, // 16:9		->		0x00, // Info		->		0x00, // VTX1		->		0x00, // VTX2		->		0x00, // VTX3		->		0x00, // Blue		->		0x00, // Yello	->		0x00, // Green	->		0x00, // Red		->		0x00, // TV			->		0x00	// Power	->	},
