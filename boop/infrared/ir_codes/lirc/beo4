 Prog+	->	Prog+	0x011f, // Prog-	->	Prog-	0x0101, // 1		->	1	0x0102, // 2		->	2	0x0103, // 3		->	3	0x0104, // 4		->	4	0x0105, // 5		->	5	0x0106, // 6		->	6	0x0107, // 7		->	7	0x0108, // 8		->	8	0x0109, // 9		->	9	0x0100, // 0		->	0	0x0000, // -/--		->	Guide	0x0183, // AV		->	Source		Audio-AUX	0x015c, // Menu		->	Menue	0x0000, // PiP		->	Tools	0x0000, // A/B		->	Return	0x0000, // 16:9		->	P.Size	0x0000, // Info		->	Info	0x0135, // VTX1		->	Ch.List		Stop	0x0136, // VTX2		->				Stop	0x0000, // VTX3		->	Pre-CH	0x0193, // Blue		->	Blue		Phono	0x0191, // Yello	->	Yellow		Tape	0x0192, // Green	->	Green		CD	0x0181, // Red		->	Red			Radio	0x010c, // TV		->	TV/DTV	0x0f0c  // Power	->	Standby},